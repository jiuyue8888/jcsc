<!--index.wxml-->
<view class="main">
  <block wx:for="{{list}}" wx:key="{{index}}">
    <view class="order-item">
      <image src="{{item.img}}"/>
      <view class="title">{{item.title}}</view>
      <view class="time">{{item.time}}</view>
      <view class="content">{{item.content}}</view>
    </view>
  </block>
</view>
